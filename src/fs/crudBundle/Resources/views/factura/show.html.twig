{% extends '::base.html.twig' %}

{% block body -%}
    <h1>factura</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Codefac</th>
                <td>{{ entity.codefac }}</td>
            </tr>
            <tr>
                <th>Idtercero</th>
                <td>{{ entity.idtercero }}</td>
            </tr>
            <tr>
                <th>Fechafac</th>
                <td>{{ entity.fechafac|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('factura') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('factura_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('factura_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
    <li>
        <a href="{{ path('itemsfactura') }}">
            NUEVO ITEM
        </a>
    </li>
    <hr>
    <li>
        <a href="{{ path('tercero') }}">
            NUEVO TERCERO
        </a>
    </li>
    
</ul>
{% endblock %}
